{
  "name": "i18n-kit-demo",
  "version": "1.2.0",
  "private": true,
  "dependencies": {
    "i18n-cli": "git+ssh://git@github.com/rodu30/i18n-cli.git",
    "i18n-kit": "git+ssh://git@github.com:rodu30/i18n-kit.git",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-scripts": "1.0.17"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "yarn extract && yarn merge && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "messages:extract": "node_modules/.bin/i18n ext src en-US src/messages",
    "messages:merge": "node_modules/.bin/i18n merge -r src/messages/en-US.json src/messages",
    "messages:export": "node_modules/.bin/i18n convert -o src/messages src/phraseApp/out",
    "messages:import":
      "node_modules/.bin/i18n convert -i src/phraseApp/in src/messages && node_modules/.bin/i18n merge -r src/messages/en-US.json src/messages"
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
